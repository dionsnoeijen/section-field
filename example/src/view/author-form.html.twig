{{ form_start(form) }}
<div class="row">
    <div class="col-md-12">
        {{ form_errors(form) }}
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        {{ form_row(form.name) }}
    </div>
    <div class="col-md-4">
        {{ form_row(form.birthdate) }}
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        {{ form_row(form.save) }}
    </div>
</div>
{{ form_end(form) }}
